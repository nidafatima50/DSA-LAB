#include<iostream>

using namespace std;

	int main() 

	{

		int n;

		cout<<"enter the number of elements ";

		cin>>n;

		int* array=new int[n];

		

		cout<<"enter elements are ";

		for(int i=0;i<n;i++)

		{

			cin>>array[i];

		}

int even=0,odd=0;

for(int i=0;i<n;i++){

if(array[i]%2==0)

even++;

else

odd++;

}



    cout << "Number of even numbers " <<even<< endl;

    cout << "Number of odd numbers " <<odd<< endl;

    delete[]array;

    return 0;

}

